<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.isaccanedo.mapper.OrderMapper">

    <!-- add order -->
    <insert id="insert" parameterType="com.isaccanedo.model.Order" useGeneratedKeys="true" keyProperty="id">
      insert into order_master (order_num,game,device,customer_name)
      values (#{orderNum}, #{game}, #{device}, #{customerName})
    </insert>

</mapper>